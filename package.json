{
  "name": "playground-react-profiler",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://ivan-ha.github.io/playground-react-profiler",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivan-ha/playground-react-profiler.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "git-hook": "lint-staged",
    "precommit": "yarn git-hook",
    "prepush": "yarn git-hook",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "antd": "^3.9.2",
    "faker": "^4.1.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-scripts": "1.1.5"
  },
  "devDependencies": {
    "gh-pages": "^1.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2"
  }
}
